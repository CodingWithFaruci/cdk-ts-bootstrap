{
    "name": "cdk-ts-bootstrap",
    "version": "0.1.0",
    "bin": {
        "cdk-ts-bootstrap": "bin/cdk-ts-bootstrap.js"
    },
    "scripts": {
        "lint": "npx eslint **/*.ts --fix --quiet",
        "build": "npx rimraf dist & tsc",
        "build-w": "npx rimraf dist & webpack",
        "watch": "tsc -w",
        "test": "jest --passWithNoTests",
        "diff-dev": "npx cdk diff cdk-ts-bootstrap-dev --ci --require-approval never --no-asset-metadata",
        "diff-tst": "npx cdk diff cdk-ts-bootstrap-tst --ci --require-approval never --no-asset-metadata",
        "diff-prd": "npx cdk diff cdk-ts-bootstrap-prd --ci --require-approval never --no-asset-metadata",
        "deploy-dev": "npx cdk deploy cdk-ts-bootstrap-dev --ci --require-approval never --no-asset-metadata",
        "deploy-tst": "npx cdk deploy cdk-ts-bootstrap-tst --ci --require-approval never --no-asset-metadata",
        "deploy-prd": "npx cdk deploy cdk-ts-bootstrap-prd --ci --require-approval never --no-asset-metadata"
    },
    "devDependencies": {
        "@octokit/rest": "^18.12.0",
        "@octokit/types": "^6.34.0",
        "@types/jest": "^28.1.0",
        "@types/libsodium-wrappers": "^0.7.9",
        "@types/node": "^17.0.38",
        "@types/yargs": "^17.0.10",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "acorn": "^8.7.1",
        "ajv": "^8.11.0",
        "ajv-keywords": "^5.1.0",
        "aws-cdk": "2.26.0",
        "camelcase": "^6.3.0",
        "dotenv": "^16.0.1",
        "eslint": "^8.16.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-plugin-jest": "^26.4.6",
        "jest": "^28.1.0",
        "jest-sonar": "^0.2.12",
        "libsodium-wrappers": "^0.7.10",
        "replace-in-file": "^6.3.5",
        "simple-git": "^3.7.1",
        "standard-version": "^9.5.0",
        "ts-jest": "^28.0.3",
        "ts-loader": "^9.3.0",
        "ts-node": "^10.8.0",
        "typescript": "~4.7.2",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.9.2",
        "yargs": "^17.5.1"
    },
    "dependencies": {
        "aws-cdk-lib": "2.26.0",
        "constructs": "^10.1.25",
        "source-map-support": "^0.5.21"
    }
}