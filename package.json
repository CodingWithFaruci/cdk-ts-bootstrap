{
    "name": "cdk-ts-bootstrap",
    "version": "0.1.0",
    "bin": {
        "cdk-ts-bootstrap": "bin/cdk-ts-bootstrap.js"
    },
    "scripts": {
        "lint": "npx eslint **/*.ts --fix --quiet",
        "build": "npx rimraf dist & tsc",
        "build-w": "npx rimraf dist & webpack",
        "watch": "tsc -w",
        "test": "jest --passWithNoTests",
        "diff-dev": "npx cdk diff cdk-ts-bootstrap-dev --ci --require-approval never --no-asset-metadata",
        "diff-tst": "npx cdk diff aws-ts-cdk-bootstrap-tst --ci --require-approval never --no-asset-metadata",
        "diff-prd": "npx cdk diff aws-ts-cdk-bootstrap-prd --ci --require-approval never --no-asset-metadata",
        "deploy-dev": "npx cdk deploy cdk-ts-bootstrap-dev --ci --require-approval never --no-asset-metadata",
        "deploy-tst": "npx cdk deploy aws-ts-cdk-bootstrap-tst --ci --require-approval never --no-asset-metadata",
        "deploy-prd": "npx cdk deploy aws-ts-cdk-bootstrap-prd --ci --require-approval never --no-asset-metadata"
    },
    "devDependencies": {
        "@octokit/rest": "^18.12.0",
        "@octokit/types": "^6.34.0",
        "@types/jest": "^26.0.10",
        "@types/libsodium-wrappers": "^0.7.9",
        "@types/node": "10.17.27",
        "@types/yargs": "^17.0.10",
        "@typescript-eslint/eslint-plugin": "^5.20.0",
        "@typescript-eslint/parser": "^5.20.0",
        "acorn": "^8.7.1",
        "ajv": "^8.11.0",
        "ajv-keywords": "^5.1.0",
        "aws-cdk": "2.21.1",
        "camelcase": "^6.3.0",
        "dotenv": "^16.0.0",
        "eslint": "^8.14.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-plugin-jest": "^26.1.5",
        "jest": "^26.4.2",
        "jest-sonar": "^0.2.12",
        "replace-in-file": "^6.3.2",
        "simple-git": "^3.7.1",
        "standard-version": "^9.3.2",
        "ts-jest": "^26.2.0",
        "ts-loader": "^9.2.8",
        "ts-node": "^9.0.0",
        "tweetsodium": "^0.0.6",
        "typescript": "~3.9.7",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2",
        "yargs": "^17.4.1"
    },
    "dependencies": {
        "aws-cdk-lib": "2.21.1",
        "constructs": "^10.0.0",
        "source-map-support": "^0.5.16"
    }
}
